##
## Podfile for the GeoConfess project.
##
## Podfile syntax reference:
## https://guides.cocoapods.org/syntax/podfile.html
##

# GeoConfess uses iOS 8 as minimum version.
platform :ios, '8.0'

# Use frameworks instead of static libraries for Pods.
# This is *required* for Swift-based projects.
use_frameworks!

# The Podfile retrieves specs from the given list of repositories.
source 'https://github.com/CocoaPods/Specs.git'

# App's dependencies.
target 'GeoConfess' do
	pod 'AWSS3',                     '~> 2.4.3'
	pod 'AWSMobileAnalytics',        '~> 2.4.3'
	pod 'AWSCognito',                '~> 2.4.3'
	pod 'Alamofire',                 '~> 3.0'
	pod 'SwiftyJSON',                '~> 2.3.2'
	pod 'GoogleMaps',                '~> 1.13'
	pod 'SideMenu',                  '~> 1.0.3'
	pod 'MBProgressHUD',             '~> 0.9.2'
	pod 'DownPicker',                '~> 0.1.34'
	pod 'TNSwiftyCheckboxGroup',     '~> 1.0.1'
	pod 'MIBadgeButton-Swift',       '~> 0.3'
	pod 'APAddressBook/Swift',       '~> 0.2.3'
	pod 'SESlideTableViewCell',      '~> 0.7.0'
	pod 'JSQMessagesViewController', '~> 7.3.1'
	pod 'Firebase/Messaging',        '~> 3.2.1'
end
